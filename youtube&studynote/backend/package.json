{
  "name": "roadmap-admin-server",
  "version": "1.0.0",
  "description": "Node.js server for managing roadmap content",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:start": "NODE_ENV=development node server.js",
    "dev:watch": "NODE_ENV=development nodemon server.js",
    "staging": "NODE_ENV=staging node server.js",
    "staging:watch": "NODE_ENV=staging nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "build": "echo 'Build complete - no compilation needed for Node.js'",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:prod": "NODE_ENV=production npm run build"
  },
  "keywords": [
    "roadmap",
    "admin",
    "express"
  ],
  "author": "Coding Terminals",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
